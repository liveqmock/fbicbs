  <select id="selectForGlcnum" resultMap="glfForGlcnumResultMap">
      <![CDATA[
      select
          distinct
          t.curcde
          ,t2.curnmc
          ,t.glcode
          ,t1.glcnam
      from actglf t join  actglc t1 on
          t.glcode=t1.glcode
          join actccy t2 on
          t.curcde=t2.curcde
      where t.rectyp='D'
          and t.orgidt=#{orgidt}
          and t.gldate between to_date(to_char((select crndat from actsct where sctnum=8),'yyyy-mm'),'yyyy-mm')
              and last_day(to_date(to_char((select crndat from actsct where sctnum=8),'yyyy-mm'),'yyyy-mm'))
      order by t.curcde,t.glcode
      ]]>
  </select>
