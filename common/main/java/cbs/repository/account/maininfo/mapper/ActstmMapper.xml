    <select id="selectForActnum2" resultMap="stmForActnumResultMap">
    <!--
        createBy: haiyuhuang
        date:    2011-04-22
        remark:用来进行报表表头每个账号的基本信息查询
        -->
        <![CDATA[
        select DISTINCT
            (select irt.irtval from actirt irt
             where (irt.irtkd1||irt.irtkd2)=act.cinrat
                   and irt.curcde=act.curcde and irt.curflg='1'
            ) as irtval
            ,nsm.sysidt
            ,nsm.cusidt
            ,nsm.orgidt
            ,org.orgnam
            ,nsm.apcode
            ,glc.glcnam
            ,ani.oldacn
            ,act.actnam
            ,act.depnum
            ,dep.depnam
            ,nsm.curcde
            ,ccy.curnmc
        from
            actstm nsm join actorg org on
            nsm.orgidt = org.orgidt
            join actapc apc on
            nsm.apcode=apc.apcode
            join actglc glc on
            apc.glcode=glc.glcode
            join actani ani on
            nsm.sysidt=ani.sysidt
            and nsm.orgidt=ani.orgidt
            and nsm.cusidt=ani.cusidt
            and nsm.apcode=ani.apcode
            and nsm.curcde=ani.curcde
            join actact act on
            nsm.sysidt=act.sysidt
            and nsm.orgidt=act.orgidt
            and nsm.cusidt=act.cusidt
            and nsm.apcode=act.apcode
            and nsm.curcde=act.curcde
            join actdep dep on
            act.orgidt=dep.orgidt
            and act.depnum=dep.depcde
            join actccy ccy on
            nsm.curcde=ccy.curcde
        where
             ani.oldacn=#{oldacn}
        order by cusidt
       ]]>
    </select>
    <select id="selectForActnum" resultMap="stmForActnumResultMap">
    <!--
        createBy: haiyuhuang
        date:    2010-12-16
        remark:用来进行报表表头每个账号的基本信息查询
        -->
        <![CDATA[
        select DISTINCT
            stm.sysidt
            ,stm.cusidt
            ,stm.orgidt
            ,org.orgnam
            ,stm.apcode
            ,glc.glcnam
            ,ani.oldacn
            ,act.actnam
            ,act.depnum
            ,dep.depnam
            ,stm.curcde
            ,ccy.curnmc
            ,lbl.legbal
        from
            actstm stm join actorg org on
            stm.orgidt = org.orgidt
            join actapc apc on
            stm.apcode=apc.apcode
            join actglc glc on
            apc.glcode=glc.glcode
            join actani ani on
            stm.sysidt=ani.sysidt
            and stm.orgidt=ani.orgidt
            and stm.cusidt=ani.cusidt
            and stm.apcode=ani.apcode
            and stm.curcde=ani.curcde
            join actact act on
            stm.sysidt=act.sysidt
            and stm.orgidt=act.orgidt
            and stm.cusidt=act.cusidt
            and stm.apcode=act.apcode
            and stm.curcde=act.curcde
            join actdep dep on
            act.orgidt=dep.orgidt
            and act.depnum=dep.depcde
            join actccy ccy on
            stm.curcde=ccy.curcde
            join actlbl lbl on
            stm.sysidt=lbl.sysidt
            and stm.orgidt=lbl.orgidt
            and stm.cusidt=lbl.cusidt
            and stm.apcode=lbl.apcode
            and stm.curcde=lbl.curcde
             and nvl(trim(stm.secccy), ' ')=nvl(trim(lbl.secccy), ' ')
        where
             stm.orgidt=#{orgidt}
        order by cusidt
       ]]>
    </select>


