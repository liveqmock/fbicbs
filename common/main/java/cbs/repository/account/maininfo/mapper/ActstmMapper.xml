    <select id="selectForStmContent2" resultMap="stmForContentResultMap">
    <!--
        createBy: haiyuhuang
        date:     2011-04-22
        remark:   分户账报表账户别传票细则查询
        -->
        <![CDATA[
        SELECT
            (TLRNUM||VCHSET||SETSEQ) AS CPCODE
            ,(CASE WHEN TXNAMT<0 THEN TXNAMT ELSE 0 END) AS DTXNAMT
            ,(CASE WHEN TXNAMT>=0 THEN TXNAMT ELSE 0 END) AS CTXNAMT
            ,ACTBAL
            ,LASBAL
            ,VALDAT
            ,FURINF
            ,THRREF
            ,(VALDAT-(SELECT SCT.IPYDAT FROM ACTSCT SCT WHERE SCT.SCTNUM=8)) AS DAYNUM
            ,CRACCM
            ,STMPNY,NSTMPG,PAGLIN
        FROM ACTSTM STM
        WHERE
            SYSIDT=#{sysidt}
            AND ORGIDT=#{orgidt}
            AND CUSIDT=#{cusidt}
            AND APCODE=#{apcode}
            AND CURCDE=#{curcde}
            AND STMPNY = #{stmpny}
            AND NSTMPG >= #{bnstmpg}
            AND NSTMPG <= (CASE WHEN #{enstmpg}=0 THEN 99999 ELSE #{enstmpg} END)

        ORDER BY STMPNY,NSTMPG,PAGLIN
        ]]>
    </select>

